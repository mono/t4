{
  "version": "2.2.0-preview.{height}",
  "publicReleaseRefSpec": [
    "^refs/tags/v\\d+\\.\\d+$",
    "^refs/tags/v\\d+\\.\\d+\\.\\d+$"
  ],
  "cloudBuild": {
    "buildNumber": {
      "enabled": true
    }
  },
  "release": {
    "branchName" : "release/v{version}",
    "versionIncrement" : "minor",
    "firstUnstableTag" : "preview"
  }
}
